# Set Background - Pick New
# exec_always --no-startup-id bgset new -c 483447

# Launch Polybar
exec_always --no-startup-id $HOME/.config/polybar/launch.sh compact

# Restore Background
exec_always --no-startup-id bgset restore

# Start Conky
exec_always --no-startup-id ~/.config/conky/run.sh



# Set display resolution
exec_always --no-startup-id xrandr --output DVI-D-0 --rotate left --right-of HDMI-0 --output DP-0 --left-of HDMI-0 --rotate right

# Start Composite Manager
exec_always --no-startup-id xcompmgr -C